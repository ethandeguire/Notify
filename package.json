{
  "name": "NotifyMe",
  "version": "0.1.0",
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "faunadb": "^2.5.2",
    "netlify-lambda": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "gatsby": "^2.13.64",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.6",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-sharp": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.11",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "bootstrap": "node ./functions/scripts/bootstrap-fauna-database.js",
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "checkForFaunaKey": "node ./functions/scripts/check-for-fauna-key.js",
    "start": "npm-run-all --parallel checkForFaunaKey start:app start:server build:gatsby",
    "start:server": "netlify-lambda serve functions -c ./webpack.config.js",
    "prebuild": "echo 'setup faunaDB' && npm install && npm run bootstrap",
    "build": "npm-run-all --parallel build:**",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "build:gatsby": "gatsby build"
  }
}
